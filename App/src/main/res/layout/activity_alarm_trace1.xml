<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        android:id="@+id/title_alarm_trace"
        layout="@layout/layout_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height" />

    <TextView
        android:id="@+id/tv_empty_tip2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="8dp"
        android:text="暂无维修人员！"
        android:visibility="gone" />

        <TextView
            android:id="@+id/tv_ensure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/selector_plan_lanse"
            android:textColor="@color/white"
            android:layout_gravity="center"
            android:visibility="gone"
            android:text="确认完成"
            android:layout_margin="@dimen/size_10dp"
            android:padding="@dimen/size_10dp"/>
    <View
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/lessgray"/>

    <ListView
        android:id="@+id/worker_listView"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:visibility="gone" />

    <com.baidu.mapapi.map.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />
    <ListView
        android:id="@+id/alarm_listView"
        android:layout_width="match_parent"
        android:layout_height="150dp" />

    <TextView
        android:id="@+id/tv_empty_tip1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="8dp"
        android:text="暂无处理中的报警！"
        android:visibility="gone" />

</LinearLayout>