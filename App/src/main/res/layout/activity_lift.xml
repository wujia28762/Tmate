<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.honyum.elevatorMan.activity.maintenance.LiftActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include
            layout="@layout/layout_title"
            android:id="@+id/title_lift"
            android:layout_width="match_parent"
            android:layout_height="@dimen/title_height"/>

    </RelativeLayout>




    <ViewStub
        android:id="@+id/tv_tip_inflate"
        android:inflatedId="@+id/tv_tip"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout="@layout/tip_layout"
       />

    <ListView
        android:id="@+id/lv_lift"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ></ListView>

</LinearLayout>
